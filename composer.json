{
    "name": "pacturan/hello-world",
    "description": "gwapong coding style",
    "license": "MIT",
    "authors": [
        {
            "name": "Gil Pacturan",
            "email": "gilpacturan@gmail.com",
            "homepage": "https://www.linkedin.com/in/gil-lopez-pacturan-460b0a1a7/"
        },
        {
            "name": "Fligno",
            "email": "apps@fligno.com",
            "homepage": "https://fligno.com/"
        }
    ],
    "homepage": "https://github.com/pacturan/hello-world",
    "keywords": ["Laravel", "HelloWorld"],
    "require": {
        "php": "^8.0",
        "illuminate/support": "~7|~8|~9",
        "luchavez/starter-kit": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6|~7"
    },
    "suggest": {
        "luchavez/starter-kit": "^1.0",
        "luchavez/boilerplate-generator": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Pacturan\\HelloWorld\\": "src/",
            "Pacturan\\HelloWorld\\Database\\Seeders\\": "database/seeders/",
            "Pacturan\\HelloWorld\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pacturan\\HelloWorld\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pacturan\\HelloWorld\\Providers\\HelloWorldServiceProvider"
            ],
            "aliases": {
                "HelloWorld": "Pacturan\\HelloWorld\\Facades\\HelloWorld"
            }
        },
        "branch-alias": {
            "dev-main": "1.0.x-dev",
            "dev-master": "1.0.x-dev",
            "dev-develop": "1.0.x-dev",
            "dev-feature": "1.0.x-dev"
        }
    }
}
